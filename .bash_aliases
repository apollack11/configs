# git aliases
alias gf='git fetch'
alias gs='git status'
alias ga='git add'
alias gaa='git add -A'
alias gc='git commit -m '
alias gd='git diff'
alias gl='gitlog'

# easy vim alias
alias v='vim'

# cd up
alias u='cd ..'
alias uu='cd ../..'
alias uuu='cd ../../..'

# set bash completion to be case insensitive (from Jarvis)
bind 'set completion-ignore-case on'

# set editor to vim for editing commands
export VISUAL=vim
export EDITOR=vim
